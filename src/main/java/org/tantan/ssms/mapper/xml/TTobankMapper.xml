<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tantan.ssms.mapper.TTobankMapper">
	<!--开启二级缓存-->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<resultMap id="BaseResultMap" type="org.tantan.ssms.entity.TTobank">
		<id column="Sta_Id" property="staId" />
		<result column="Sta_IdentityNum" property="staIdentitynum" />
		<result column="Sta_BankAcount" property="staBankacount" />
		<result column="Pr_FullBonus" property="prFullbonus" />
		<result column="TB_TatalMoney" property="tbTatalmoney" />
		<result column="TB_Time" property="tbTime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        Sta_Id AS staId, Sta_IdentityNum AS staIdentitynum, Sta_BankAcount AS staBankacount, Pr_FullBonus AS prFullbonus, TB_TatalMoney AS tbTatalmoney, TB_Time AS tbTime
    </sql>
</mapper>
