<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tantan.ssms.mapper.TInsuranceMapper">
	<!--开启二级缓存-->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<resultMap id="BaseResultMap" type="org.tantan.ssms.entity.TInsurance">
		<id column="Sta_Id" property="staId" />
		<result column="Ins_Medical" property="insMedical" />
		<result column="Ins_Aged" property="insAged" />
		<result column="Ins_Unemp" property="insUnemp" />
		<result column="Ins_Accu" property="insAccu" />
		<result column="Ins_Total" property="insTotal" />
		<result column="Ins_Time" property="insTime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        Sta_Id AS staId, Ins_Medical AS insMedical, Ins_Aged AS insAged, Ins_Unemp AS insUnemp, Ins_Accu AS insAccu, Ins_Total AS insTotal, Ins_Time AS insTime
    </sql>
</mapper>
