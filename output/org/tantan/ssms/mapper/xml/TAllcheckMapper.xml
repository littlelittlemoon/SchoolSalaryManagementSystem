<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tantan.ssms.mapper.TAllcheckMapper">
	<!--开启二级缓存-->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<resultMap id="BaseResultMap" type="org.tantan.ssms.entity.TAllcheck">
		<id column="Sta_Id" property="staId" />
		<result column="AC_DueToDays" property="acDuetodays" />
		<result column="AC_ActualDays" property="acActualdays" />
		<result column="AC_Money" property="acMoney" />
		<result column="AC_Time" property="acTime" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        Sta_Id AS staId, AC_DueToDays AS acDuetodays, AC_ActualDays AS acActualdays, AC_Money AS acMoney, AC_Time AS acTime
    </sql>
</mapper>
